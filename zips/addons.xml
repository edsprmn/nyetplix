<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.nyetplix" name="Nyetplix Repository" version="1.0.0" provider-name="edsprmn">
    <extension point="xbmc.addon.repository" name="My Custom Repository">
        <dir>
            <info compressed="false">https://edsprmn.github.io/nyetplix/zips/addons.xml</info>
            <checksum>https://edsprmn.github.io/nyetplix/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://edsprmn.github.io/nyetplix/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>My Personal Kodi Addons</summary>
        <description>A repository for my own experimental Kodi plugins.</description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="plugin.video.nyetplix" name="NYETPLIX" version="2.2.0" provider-name="edsprmn">
    <requires>
        <import addon="xbmc.python" version="3.0.0"/>
        <import addon="script.module.requests" version="2.25.1"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="main.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Nonton Movie, Rebahin, Telenovela &amp; Live IPTV</summary>
        <description>NYETPLIX: Addon Streaming masa depan. Nonton film dari LK21 &amp; Rebahin, Telenovela (JAV), dan saluran TV Indonesia/Sports secara Live.</description>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
        </assets>
    </extension>
</addon>

</addons>
